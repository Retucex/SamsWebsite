<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet"
          asp-href-include="/lib/bootstrap/dist/**/*.min.css, /css/*.min.css"
          asp-href-exclude="**/*-reboot*,**/*-grid*" />
    <script asp-src-include="/lib/bootstrap/dist/**/*.bundle.min.js"></script>
    <title>@ViewBag.Title</title>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" asp-controller="Home" asp-action="Index">SLIInc.</a>
        <ul class="navbar-nav mr-auto">
            <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Blog" asp-route-blogPost="">Blog</a></li>
            <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Projects">Projects</a></li>
            <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="About">About</a></li>
        </ul>
    </nav>
    <main>
        @RenderBody()
    </main>
</body>
</html>
